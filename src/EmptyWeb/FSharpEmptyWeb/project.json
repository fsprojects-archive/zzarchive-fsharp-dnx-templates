{
  "webroot": "wwwroot",
  "version": "1.0.0-*",

  "dependencies": {
    "FSharp.Core": "4.0.0.1",
    "FSharp.Dnx": "1.0.0-*",
    "Microsoft.AspNet.IISPlatformHandler": "1.0.0-*",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-*"
  },

  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel"
  },

  "frameworks": {
    "dnx451": { },
    "dnxcore50": { }
  },

  "exclude": [
    "wwwroot",
    "node_modules"
  ],
  "publishExclude": [
    "**.user",
    "**.vspscc"
  ],

  "compiler": {
    "name": "F#",
    "compilerAssembly": "FSharp.Dnx",
    "compilerType": "FSharp.Dnx.FSharpProjectCompiler"
  },

  "fsharp": {
    "files": [
      "Properties/AssemblyInfo.fs",
      "Startup.fs"
    ]
  }
}
